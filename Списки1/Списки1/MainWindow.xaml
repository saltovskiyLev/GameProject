<Window x:Class="Списки1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Списки1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical">
            <TextBlock x:Name="Result">
        </TextBlock>
        <TextBox Width="240" Name="Data">
        </TextBox>
            <Button Click="Button_Click" Content="Добавить в список" Foreground="LightSteelBlue">
            </Button>
            <Button Click="Button_Click_1" Content="Удалить из списка">
            </Button>
        </StackPanel>
    </Grid>
</Window>
